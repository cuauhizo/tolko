{
  "$schema": "https://json.schemastore.org/prettierrc",
  "semi": false,
  "printWidth": 250,
  "singleQuote": true,
  "tabWidth": 2,
  "useTabs": false,
  "bracketSameLine": true,
  "bracketSpacing": true,
  "singleAttributePerLine": false,
  "vueIndentScriptAndStyle": false,
  "arrowParens": "avoid",
  "htmlWhitespaceSensitivity": "ignore",
  "endOfLine": "lf",
  "trailingComma": "es5"
}
